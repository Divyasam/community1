<div ng-controller = "addProjectCtrl">

<div class="hold-transition skin-blue sidebar-mini">
  <div>
    <section class="content">
	<div class="row">
		<div class="col-lg-5 col-xs-5">
		</div>
		<div class="col-lg-4 col-xs-4">
			<h1> Add New Projects </h1>
			<br>
		</div>
		<div class="col-lg-4 col-xs-4">
		</div>
	</div>
	<form>
      <div class="row ">
	  <section>
	  <div class="col-lg-3 col-xs-3">
	  </div>
	  		<div class="col-lg-2 col-xs-2">
			<label><h4><strong>Project Name:</strong></h4></label>
		</div>
		<div class="col-lg-4 col-xs-4">
			<input class="form-control" type="text" id="projectName" style="border:2px solid #757575;" name="projectName" placeholder="Project Name" ng-model="projectName" required />
		</div>
		</section>
		</div>

	    <hr/>
		<div class="row">
		<div class="col-lg-3 col-xs-3">
	  </div>
		<div class="col-lg-2 col-xs-1">
			<label><h4><strong>Project Description:</strong></h4></label>
		</div>
		<div class="col-lg-4 col-xs-4">
			<input class="form-control" style="border:2px solid #757575;" type="text" id="description" name="description" placeholder="Description" ng-model="description" required />
		</div>
		</div>

		 <hr/>

	<div class="row">
		<div class="col-lg-3 col-xs-3">
		</div>
		<div class="col-lg-2 col-xs-2">
			<label><h4><strong>Development Type:</strong></h4></label>
		</div>
		<div class="col-lg-4 col-xs-4">

			<select name="testingType" id="testingType" style="border:2px solid #757575;" ng-model="testingType" required class="form-control">
				<option value="">-Type of Development-</option>
				<option value="Usability Testing">Applications Development</option>
				<option value="Compatibility Testing">Systems Development</option>
				<option value="Interface Testing">Web Development</option>
				<option value="Interface Testing">Embedded Systems Development</option>
				<option value="Operational Testing">Scientific Development</option>
				<option value="Installation Testing">Test Automation</option>
			</select>

		</div>
	</div>
	<hr/>




<div class="row">

	<div class="col-lg-3 col-xs-3">
	</div>
	<div class="col-lg-2 col-xs-2">
		<label><h4><strong>Choose the Platform:</strong></h4></label>
	</div>
	<div class="col-lg-4 col-xs-4">

		<select name="platform" style="border:2px solid #757575;" id="platform" ng-model="platform" required class="form-control">
			<option value="">-Platform-</option>
			<option value="iOS">.Net</option>
			<option value="Android">C++</option>
			<option value="Blackberry">C#</option>
			<option value="MeeGo">Java</option>
			<option value="Palm">Python</option>
		</select>

	</div>




	</div>
<hr/>


<hr/>

<div class="row">

		<div class="col-lg-4 col-xs-4">
		</div>
		<div class="col-lg-2 col-xs-2">
		</div>
		<div class="col-lg-2 col-xs-2">
			<input type="submit" class="btn btn-primary" value="CREATE" ng-click="addProject()"></input>
		</div>
		<div class="col-lg-4 col-xs-4">
		</div>
	</div>

	<div class="alert alert-success col-md-12" id="success-alert2" style="display: none ">
	<center> <strong> Successfully Added Project! Please check the list of projects</strong></center>
	</div>
		</form>
	</div>
</div>
</div>
</div>


